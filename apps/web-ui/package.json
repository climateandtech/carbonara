{
  "name": "@carbonara/web-ui",
  "version": "1.0.0",
  "description": "Carbonara Svelte Web UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
