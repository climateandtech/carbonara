  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "tslib": "^2.6.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
