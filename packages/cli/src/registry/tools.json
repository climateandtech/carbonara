    {
      "id": "if-cpu-metrics",
      "name": "IF CPU Metrics",
      "description": "Monitor local CPU utilization and energy consumption while analyzing web applications",
      "command": {
        "executable": "if-run",
        "args": ["--manifest", "{manifest}", "--output", "{output}"],
        "outputFormat": "yaml"
      },
      "installation": {
        "type": "npm",
        "package": "@grnsft/if @tngtech/if-webpage-plugins",
        "global": true,
        "instructions": "npm install -g @grnsft/if && npm install -g @tngtech/if-webpage-plugins"
      },
      "detection": {
        "method": "command",
        "target": "npm list -g @tngtech/if-webpage-plugins && if-run --help"
      },
      "parameters": [
        {
          "name": "thermal-design-power",
          "required": false,
          "type": "number",
          "description": "CPU thermal design power in watts"
        },
        {
          "name": "vcpus-total",
          "required": false,
          "type": "number",
          "description": "Total number of vCPUs"
        }
      ],
      "manifestTemplate": {
        "name": "cpu-metrics-monitoring",
        "description": "Monitor local CPU utilization during web application analysis",
        "initialize": {
          "outputs": ["yaml"],
          "plugins": {
            "cpu-metrics": {
              "method": "MeasureCpu",
              "path": "@tngtech/if-webpage-plugins",
              "config": {}
            },
            "interpolate": {
              "path": "builtin",
              "method": "Interpolation",
              "config": {
                "method": "linear",
                "x": [0, 10, 50, 100],
                "y": [0.12, 0.32, 0.75, 1.02],
                "input-parameter": "cpu/utilization",
                "output-parameter": "cpu-factor"
              }
            },
            "cpu-factor-to-wattage": {
              "path": "builtin",
              "method": "Multiply",
              "config": {
                "input-parameters": ["cpu-factor", "thermal-design-power"],
                "output-parameter": "cpu-wattage"
              }
            },
            "wattage-times-duration": {
              "path": "builtin",
              "method": "Multiply",
              "config": {
                "input-parameters": ["cpu-wattage", "duration"],
                "output-parameter": "cpu-wattage-times-duration"
              }
            },
            "wattage-to-energy-kwh": {
              "path": "builtin",
              "method": "Divide",
              "config": {
                "numerator": "cpu-wattage-times-duration",
                "denominator": 3600000,
                "output": "cpu-energy-raw"
              }
            },
            "calculate-vcpu-ratio": {
              "path": "builtin",
              "method": "Divide",
              "config": {
                "numerator": "vcpus-total",
                "denominator": "vcpus-allocated",
                "output": "vcpu-ratio"
              }
            },
            "correct-cpu-energy-for-vcpu-ratio": {
              "path": "builtin",
              "method": "Divide",
              "config": {
                "numerator": "cpu-energy-raw",
                "denominator": "vcpu-ratio",
                "output": "cpu/energy"
              }
            },
            "energy-to-carbon": {
              "path": "builtin",
              "method": "Multiply",
              "config": {
                "input-parameters": ["grid-carbon-intensity", "cpu/energy"],
                "output-parameter": "carbon"
              }
            }
          }
        },
        "tree": {
          "children": {
            "child": {
              "pipeline": {
                "observe": ["cpu-metrics"],
                "regroup": [],
                "compute": [
                  "interpolate",
                  "cpu-factor-to-wattage",
                  "wattage-times-duration",
                  "wattage-to-energy-kwh",
                  "calculate-vcpu-ratio",
                  "correct-cpu-energy-for-vcpu-ratio",
                  "energy-to-carbon"
                ]
              },
              "defaults": {
                "thermal-design-power": 100,
                "vcpus-total": 8,
                "vcpus-allocated": 2,
                "grid-carbon-intensity": 750
              },
              "inputs": [{}]
            }
          }
        }
      },
      "display": {
        "category": "Impact Analysis",
        "icon": "üíª",
        "groupName": "IF CPU Analysis",
        "entryTemplate": "üíª Local CPU Analysis - {date}",
        "descriptionTemplate": "CPU: {cpuUtilization}%, Energy: {energy} kWh",
        "fields": [
          {
            "key": "cpuUtilization",
            "label": "üíª CPU Utilization",
            "path": "data.tree.children.child.outputs[0]['cpu/utilization']",
            "type": "number",
            "format": "{value}%"
          },
          {
            "key": "energy",
            "label": "‚ö° Energy",
            "path": "data.tree.children.child.outputs[0]['cpu/energy']",
            "type": "energy",
            "format": "{value} kWh"
          },
          {
            "key": "carbon",
            "label": "üå± CO2 Emissions",
            "path": "data.tree.children.child.outputs[0]['carbon']",
            "type": "carbon",
            "format": "{value}g"
          }
        ]
      },
      "options": [
        {
          "flag": "-s, --save",
          "description": "Save results to data lake",
          "type": "boolean"
        },
        {
          "flag": "-o, --output <format>",
          "description": "Output format (json|table)",
          "type": "string",
          "default": "table"
        }
      ]
    },
